GEONAMES_INFO_FILE=readme.txt
GEONAMES_FEATURE_CODES_FILE=featureCodes_en.txt
GEONAMES_LANGUAGE_CODES_FILE=iso-languagecodes.txt
GEONAMES_COUNTRIES_FILE=countryInfo.txt
