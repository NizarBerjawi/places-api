GEONAMES_COUNTRIES_FILE=countryInfo.txt
GEONAMES_TIME_ZONES_FILE=timeZones.txt
GEONAMES_ALTERNATE_NAMES_ZIP_FILE=alternateNamesV2.zip


GEONAMES_DATE_PLACEHOLDER="<<date>>"
GEONAMES_MODIFICATIONS_FILE=modifications-${GEONAMES_DATE_PLACEHOLDER}.txt
GEONAMES_DELETES_FILE=deletes-${GEONAMES_DATE_PLACEHOLDER}.txt
GEONAMES_ALTERNATE_NAMES_MODIFICATIONS_FILE=alternateNamesModifications-${GEONAMES_DATE_PLACEHOLDER}.txt
GEONAMES_ALTERNATE_NAMES_DELETES_FILE=alternateNamesDeletes-${GEONAMES_DATE_PLACEHOLDER}.txt

GEONAMES_FLAGS_URL="${GEONAMES_IMAGES_URL}/flags/x"
