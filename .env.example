APP_NAME="Places API"
APP_ENV=local
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost:8080
APP_TIMEZONE=UTC

LOG_CHANNEL=stack
LOG_SLACK_WEBHOOK_URL=

DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=places_api_database
DB_USERNAME=places_api
DB_PASSWORD=secret

CACHE_DRIVER=file
QUEUE_CONNECTION=database

