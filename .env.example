GEONAMES_FLAGS_URL="${GEONAMES_IMAGES_URL}/flags/x"
GEONAMES_FILES_URL="${GEONAMES_DOWNLOADS_URL}/export/dump"

GEONAMES_LANGUAGE_CODES_URL="${GEONAMES_FILES_URL}/${GEONAMES_LANGUAGE_CODES_FILE}"
GEONAMES_COUNTRIES_URL="${GEONAMES_FILES_URL}/${GEONAMES_COUNTRIES_FILE}"
GEONAMES_README_URL="${GEONAMES_FILES_URL}/${GEONAMES_INFO_FILE}"
GEONAMES_TIME_ZONES_URL="${GEONAMES_FILES_URL}/${GEONAMES_TIME_ZONES_FILE}"
GEONAMES_FEATURE_CODES_URL="${GEONAMES_FILES_URL}/${GEONAMES_FEATURE_CODES_FILE}"