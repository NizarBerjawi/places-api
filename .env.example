APP_NAME=Lumen
APP_ENV=local
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost
APP_TIMEZONE=UTC

LOG_CHANNEL=stack
LOG_SLACK_WEBHOOK_URL=

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=countries_api_database
DB_USERNAME=countries_api
DB_PASSWORD=secret

CACHE_DRIVER=file
QUEUE_CONNECTION=sync

GITHUB_URL=https://github.com/NizarBerjawi/places-api

GEONAMES_PAGINATION_LIMIT=10

GEONAMES_URL=https://www.geonames.org
GEONAMES_DOWNLOADS_URL=https://download.geonames.org
