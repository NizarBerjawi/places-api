BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database
SESSION_DRIVER=file
SESSION_LIFETIME=120

RESPONSE_CACHE_ENABLED=true
RESPONSE_CACHE_HEADER_NAME=laravel-responsecache
RESPONSE_CACHE_DRIVER=file
